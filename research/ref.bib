@article{lourencoWhoseAlgorithmSays2020,
	title = {Whose {Algorithm} {Says} {So}: {The} {Relationships} {Between} {Type} of {Firm}, {Perceptions} of {Trust} and {Expertise}, and the {Acceptance} of {Financial} {Robo}-{Advice}},
