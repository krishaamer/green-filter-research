@article{lentonQuantifyingHumanCost2023,
  title = {Quantifying the Human Cost of Global Warming},
  author = {Lenton, Timothy M. and Xu, Chi and Abrams, Jesse F. and Ghadiali, Ashish and Loriani, Sina and Sakschewski, Boris and Zimm, Caroline and Ebi, Kristie L. and Dunn, Robert R. and Svenning, Jens-Christian and Scheffer, Marten},
  year = {2023},
  month = may,
  journal = {Nature Sustainability},
  issn = {2398-9629},
  doi = {10.1038/s41893-023-01132-6},
  url = {https://www.nature.com/articles/s41893-023-01132-6},
  urldate = {2023-08-05},
  abstract = {Abstract             The costs of climate change are often estimated in monetary terms, but this raises ethical issues. Here we express them in terms of numbers of people left outside the `human climate niche'---defined as the historically highly conserved distribution of relative human population density with respect to mean annual temperature. We show that climate change has already put\,{\textasciitilde}9\% of people ({$>$}600 million) outside this niche. By end-of-century (2080--2100), current policies leading to around 2.7\,{$^\circ$}C global warming could leave one-third (22--39\%) of people outside the niche. Reducing global warming from 2.7 to 1.5\,{$^\circ$}C results in a\,{\textasciitilde}5-fold decrease in the population exposed to unprecedented heat (mean annual temperature\,{$\geq$}29\,{$^\circ$}C). The lifetime emissions of\,{\textasciitilde}3.5 global average citizens today (or\,{\textasciitilde}1.2 average US citizens) expose one future person to unprecedented heat by end-of-century. That person comes from a place where emissions today are around half of the global average. These results highlight the need for more decisive policy action to limit the human costs and inequities of climate change.},
  langid = {english},
  file = {/Users/krishaamer/Zotero/storage/AL2U4URS/Lenton et al. - 2023 - Quantifying the human cost of global warming.pdf}
}

