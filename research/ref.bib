@misc{debalieKateRaworthCreating2018,
  title = {Kate {{Raworth}}: {{Creating}} a 21st Century Economy - {{Doughnut Economics}}},
  shorttitle = {Kate {{Raworth}}},
  author = {{De Balie}},
  year = {2018},
