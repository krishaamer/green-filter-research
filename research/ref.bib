  title = {Artificial Intelligence and the Future of Life Sciences},
  author = {Leite, Michel L. and {de Loiola Costa}, Lorena S. and Cunha, Victor A. and Kreniski, Victor and {de Oliveira Braga Filho}, Mario and {da Cunha}, Nicolau B. and Costa, Fabricio F.},
