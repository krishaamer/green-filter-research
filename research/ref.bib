@article{MIYOSHI2022267,
  title = {Evaluation of Circularity of Components for Life Cycle Design: {{A}} Toner Bottle Case Study},
  author = {Miyoshi, Shion and Segawa, Takumi and Takii, Mariko and Imamura, Tsuyoshi and Sakurai, Hideo and Kurosawa, Yoshinori and Kondo, Shinsuke and Kishita, Yusuke and Umeda, Yasushi},
