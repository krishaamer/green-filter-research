@article{jollyCafeDelphiHybridising2021,
  title = {Caf\'e {{Delphi}}: {{Hybridising}} `{{World Caf\'e}}' and `{{Delphi Techniques}}' for Successful Remote Academic Collaboration},
  shorttitle = {Caf\'e {{Delphi}}},
  author = {Jolly, A. and Caulfield, L. S. and Sojka, B. and Iafrati, S. and Rees, J. and Massie, R.},
