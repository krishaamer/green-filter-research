@article{bradshawUnderestimatingChallengesAvoiding2021,
  title = {Underestimating the {{Challenges}} of {{Avoiding}} a {{Ghastly Future}}},
  author = {Bradshaw, Corey J. A. and Ehrlich, Paul R. and Beattie, Andrew and Ceballos, Gerardo and Crist, Eileen and Diamond, Joan and Dirzo, Rodolfo and Ehrlich, Anne H. and Harte, John and Harte, Mary Ellen and Pyke, Graham and Raven, Peter H. and Ripple, William J. and Saltr{\'e}, Fr{\'e}d{\'e}rik and Turnbull, Christine and Wackernagel, Mathis and Blumstein, Daniel T.},
